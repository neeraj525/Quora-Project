<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>logServlet</servlet-name>
        <servlet-class>controller.logServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WelcomeServlet</servlet-name>
        <servlet-class>controller.WelcomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signupservlet</servlet-name>
        <servlet-class>controller.signupservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QServlet</servlet-name>
        <servlet-class>controller.QServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QAServlet</servlet-name>
        <servlet-class>controller.QAServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>controller.LogOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchQ</servlet-name>
        <servlet-class>controller.SearchQ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddAServlet</servlet-name>
        <servlet-class>controller.AddAServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewAServlet</servlet-name>
        <servlet-class>controller.ViewAServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertQA</servlet-name>
        <servlet-class>controller.InsertQA</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logServlet</servlet-name>
        <url-pattern>/logServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WelcomeServlet</servlet-name>
        <url-pattern>/WelcomeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signupservlet</servlet-name>
        <url-pattern>/signupservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QServlet</servlet-name>
        <url-pattern>/QServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QAServlet</servlet-name>
        <url-pattern>/QAServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/LogOutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchQ</servlet-name>
        <url-pattern>/SearchQ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAServlet</servlet-name>
        <url-pattern>/AddAServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewAServlet</servlet-name>
        <url-pattern>/ViewAServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertQA</servlet-name>
        <url-pattern>/InsertQA</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home2.html</welcome-file>
    </welcome-file-list>
</web-app>
